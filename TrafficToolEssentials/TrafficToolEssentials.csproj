            <Private>false</Private>
        </Reference>
        <Reference Include="Colossal.UI">
            <Private>false</Private>
        </Reference>
        <Reference Include="Colossal.UI.Binding">
            <Private>false</Private>
        </Reference>
        <Reference Include="Newtonsoft.Json">
            <Private>false</Private>
        </Reference>
        <Reference Include="Unity.Burst">
            <Private>false</Private>
        </Reference>
        <Reference Include="Unity.Collections">
            <Private>false</Private>
        </Reference>
        <Reference Include="Unity.Entities">
            <Private>false</Private>
        </Reference>
        <Reference Include="Unity.InputSystem">
            <Private>false</Private>
        </Reference>
        <Reference Include="Unity.Mathematics">
            <Private>false</Private>
        </Reference>
        <Reference Include="UnityEngine.CoreModule">
            <Private>false</Private>
        </Reference>
        <Reference Include="UnityEngine.ImageConversionModule">
            <Private>false</Private>
        </Reference>
    </ItemGroup>

    <ItemGroup>
        <Reference Update="System">
            <Private>false</Private>
        </Reference>
        <Reference Update="System.Core">
            <Private>false</Private>
        </Reference>
        <Reference Update="System.Data">
            <Private>false</Private>
        </Reference>
    </ItemGroup>

    <ItemGroup>
        <None Include="$(ModPropsFile)" Link="Properties\Mod.props" />
        <None Include="$(ModTargetsFile)" Link="Properties\Mod.targets" />
    </ItemGroup>

    <ItemGroup>
        <EmbeddedResource Include="Resources\**\*" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\CommonLibraries\LaneSystem\LaneSystem.csproj" />
    </ItemGroup>

    <!-- =============================================================== -->
    <!-- OPTIMIERTES FRONTEND BUILD TARGET                                -->
    <!-- Features:                                                        -->
    <!-- - Intelligentes npm install (nur wenn noetig)                   -->
    <!-- - Cache-Clearing bei Bedarf                                     -->
    <!-- - Kann komplett deaktiviert werden                              -->
    <!-- =============================================================== -->
    <Target Name="BuildFrontend" BeforeTargets="CoreCompile" Condition="'$(DisableBuildFrontend)' != 'true'">
        <!-- Clean Frontend Cache (nur bei vollstaendigem Rebuild) -->
        <Message Text="[BuildFrontend] Pruefe Frontend Cache..." Importance="high" />
        <RemoveDir Directories="..\TLEFrontend\node_modules\.cache" Condition="'$(CleanFrontendCache)' == 'true'" />
        <Message Text="[BuildFrontend] Cache geloescht (CleanFrontendCache=true)" Condition="'$(CleanFrontendCache)' == 'true'" Importance="high" />
        
        <!-- npm install nur wenn package.json neuer als node_modules -->
        <Message Text="[BuildFrontend] Pruefe ob npm install erforderlich..." Importance="high" />
        <Exec Command="npm install --prefer-offline --no-audit" 
              WorkingDirectory="..\TLEFrontend\" 
              Condition="!Exists('..\TLEFrontend\node_modules')"
              ContinueOnError="false" />
        <Message Text="[BuildFrontend] npm install ausgefuehrt (node_modules fehlte)" Condition="!Exists('..\TLEFrontend\node_modules')" Importance="high" />
        
        <!-- Build Frontend -->
        <Message Text="[BuildFrontend] Fuehre npm run build aus..." Importance="high" />
        <Exec Command="npm run build" WorkingDirectory="..\TLEFrontend\" ContinueOnError="false" />
        <Message Text="[BuildFrontend] Frontend build erfolgreich!" Importance="high" />
        
        <!-- Copy Files -->
        <Copy SourceFiles="..\TLEFrontend\dist\C2VM.TrafficToolEssentials.mjs" DestinationFolder="$(OutDir)\" />
        <Copy SourceFiles="..\TLEFrontend\mod.json" DestinationFolder="$(OutDir)\" />
        <Copy SourceFiles="Properties\Thumbnail.png" DestinationFolder="$(OutDir)\" />
        <Message Text="[BuildFrontend] Dateien kopiert nach $(OutDir)" Importance="high" />
    </Target>

    <Target Name="CopyLaneSystem" BeforeTargets="CoreCompile">
        <ItemGroup>
            <LaneSystemFiles Include="..\CommonLibraries\LaneSystem\bin\$(Configuration)\$(TargetFramework)\*.*" />
        </ItemGroup>
