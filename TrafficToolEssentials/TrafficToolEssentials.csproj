<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <OutputType>Library</OutputType>
        <TargetFramework>net48</TargetFramework>
        <Configurations>Debug;Release</Configurations>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>

        <AssemblyName>C2VM.TrafficToolEssentials</AssemblyName>
        <RootNamespace>C2VM.TrafficToolEssentials</RootNamespace>
        <Version>2.2.2</Version>

        <!--The file where mod information which is required for publishing mod on PDX mods are stored-->
        <PublishConfigurationPath>Properties\PublishConfiguration.xml</PublishConfigurationPath>
        
        <!-- Frontend Build Kontrolle (NEU!) -->
        <DisableBuildFrontend Condition="'$(DisableBuildFrontend)' == ''">false</DisableBuildFrontend>
        <CleanFrontendCache Condition="'$(CleanFrontendCache)' == ''">false</CleanFrontendCache>
    </PropertyGroup>

    <!--Imports must be after PropertyGroup block-->
    <Import Project="$([System.Environment]::GetEnvironmentVariable('CSII_TOOLPATH', 'EnvironmentVariableTarget.User'))\Mod.props" />
    <Import Project="$([System.Environment]::GetEnvironmentVariable('CSII_TOOLPATH', 'EnvironmentVariableTarget.User'))\Mod.targets" Condition="'$(DisablePostProcessors)' != 'true'" />

    <PropertyGroup>
        <LangVersion>12.0</LangVersion>
    </PropertyGroup>

    <ItemGroup>
        <Reference Include="Game">
            <Private>false</Private>
        </Reference>
        <Reference Include="cohtml.Net">
            <Private>false</Private>
        </Reference>
